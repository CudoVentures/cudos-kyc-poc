{
    "compileOnSave": true,
    "compilerOptions": {
        "strict": true,
        "lib": ["es5", "es2015"],
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "sourceMap": true,
        "skipLibCheck": true,
        "target": "es5",
        "typeRoots": ["./node_modules/@types"],
        "noImplicitAny": false
    }
}
